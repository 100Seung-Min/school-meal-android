<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="mealItem"
            type="com.example.domain.entity.MealEntity.MealServiceDietInfo.DietRow" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/find_meal_layout">

        <TextView
            android:id="@+id/school_meal_date_txt"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:padding="5dp"
            android:id="@+id/school_meal_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/school_meal_date_txt" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>